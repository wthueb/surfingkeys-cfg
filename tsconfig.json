{
    "compilerOptions": {
        "target": "es2022",
        "lib": [
            "es2022",
            "DOM"
        ],
        "module": "es2022",
        "rootDir": "./",
        "moduleResolution": "bundler",
        "baseUrl": "./",
        "paths": {
            "config.json": [
                "./config.json"
            ]
        },
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
